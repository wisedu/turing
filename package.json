    "shelljs": "^0.7.8",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "vue": "^2.5.16",
    "vue-loader": "^10.0.0",
    "vue-router": "^3.0.1",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.8.2",
    "webpack-merge": "^4.1.0"
  }
