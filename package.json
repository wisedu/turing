  "packageName": "tg-turing",
  "main": "dist/tg-turing.min.js",
  "scripts": {
    "dev": "npm run buildcss && cross-env NODE_ENV=development webpack-dev-server --open --inline --hot --config build/webpack.dev.config.js",
    "build": "npm run buildskin && node ./build/build.js && cross-env NODE_ENV=production webpack --progress --hide-modules --config build/webpack.prod.config.js",
    "local": "npm run buildskin && node ./build/build.js && cross-env NODE_ENV=production webpack --progress --hide-modules --config build/webpack.prod.config.js && cp -r ./dist/* ../turing-antd/node_modules/tg-turing/dist/",
    "local2": "npm run buildskin && node ./build/build.js && cross-env NODE_ENV=production webpack --progress --hide-modules --config build/webpack.prod.config.js && cp -r ./dist/* ../../GitLab/egg/project-v/node_modules/tg-turing/dist/",
    "esm": "rollup --config ./build/rollup.config.js",
    "deploy": "rm -rf dist && node ./build/deploy.js",
    "buildcss": "node ./build/buildcss.js",
    "buildskin": "node ./build/buildskin.js"
  },
  "dependencies": {
    "accounting-js": "^1.1.1",
    "axios": "^0.18.0",
    "core-js": "^2.5.7",
    "moment": "^2.22.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
